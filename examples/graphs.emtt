{
  Gr : @theory = {
    V: @type;
    E: [v1: V, v2: V] -> @type;
  };

  TwoPath[G: Gr] : [x: G.V, z: G.V] -> @type = [x, z] â†¦ {
    y: G.V;
    e1: G.E[x,y];
    e2: G.E[y,z];
  };

  Joined[G: Gr] : @type = {
    x: G.V;
    z: G.V;
    p1: TwoPath[G][x,z];
    p2: TwoPath[G][z,x];
    p1.y == p2.y;
  };

  Dump[G: Gr, g: Joined[G]]: {} = {
    %dump "joined.svg";
    {}
  };
};
